<%- include('portals/header') %>


<div class="">
    <div class="col-11 m-auto my-2 shadow rounded bg-light p-4">
        <h1>Product name: <span class="px-4"><%= rewuesteditemview.productname %></span></h1>
        <h1>Product description: <span class="px-4"><%= rewuesteditemview.productdescription %></span></h1>
        <h1>Product total sell: <span class="px-4"><%= rewuesteditemview.totalsell %></span></h1>
        <h1>Product weath: <span class="px-4"><%= rewuesteditemview.weath %></span></h1>
        <h1>Paymented price: <span class="px-4"><%= viewforitemrecive.paymentedprice %></span></h1>
        <h1>Payment code: <span class="px-4"><%= viewforitemrecive.paymentcode %></span></h1>
        <h1>Phone number: <span class="px-4"><%= viewforitemrecive.phonenumber %></span></h1>
        <h1>Stutas: <span class="px-4"><%= viewforitemrecive.orderStatus %></span></h1>
        <h1>Address: <span class="px-4"><%= viewforitemrecive.address %></span></h1>
        <h1>Code: <span class="px-4"><%= viewforitemrecive.deliverycompletecode %></span></h1>
        <img style="height: 200px;" class="w-25 d-block" src="/assets/images/post/<%= rewuesteditemview.productfile %>" alt="">
        <a class="d-block m-auto w-25 text-bolder text-center text-decoration-none fs-5 my-4 py-2 px-4 bg-dark text-light" href="/confirmrequestitem/<%= viewforitemrecive._id %>">Close and accept</a>
        <a class="d-block m-auto w-25 text-bolder text-center text-decoration-none fs-5 my-4 py-2 px-4 bg-dark text-light" href="/deleterequestitem/<%= viewforitemrecive._id %>">Close and delete</a>
    </div>
</div>